{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock",
    "roots",
    "wordpress",
    "stack",
    "composer",
    "vagrant",
    "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist",
    "discard-changes": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
       "type": "vcs",
       "url" : "git@github.com:eTeamsys/wp-library.git"
    },
    {
       "type": "vcs",
       "url" : "git@github.com:eTeamsys/maisonscompere_theme.git"
    },
    {
      "type": "vcs",
      "url" : "git@github.com:eTeamsys/wpai-acf-add-on.git"
    },
    {
      "type": "vcs",
      "url" : "git@github.com:eTeamsys/wp-all-import-pro.git"
    },
    {
      "type": "vcs",
      "url" : "git@github.com:eTeamsys/wp-all-export-pro.git"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.8.0",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?a=download&p=pro&k={%ACF_PRO_KEY}&t={%version}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-multilingual-cms",
        "version": "4.3.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6088&user_id={%PLUGIN_WPML_USER_ID}&subscription_key={%PLUGIN_WPML_SUBSCRIPTION_KEY}&version={%version}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-string-translation",
        "version": "3.0.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6092&user_id={%PLUGIN_WPML_USER_ID}&subscription_key={%PLUGIN_WPML_SUBSCRIPTION_KEY}&version={%version}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-media-translation",
        "version": "2.5.4",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=7474&user_id={%PLUGIN_WPML_USER_ID}&subscription_key={%PLUGIN_WPML_SUBSCRIPTION_KEY}&version={%version}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^2.0"
        }
      }
    }
  ],
  "require": {
     "php": ">=7.1",
    "composer/installers": "^1.7",
    "vlucas/phpdotenv": "^3.4.0",
    "oscarotero/env": "^1.2.0",
    "roots/wordpress": "5.4.2",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "eteamsys/wp-library": "dev-master",
    "eteamsys/wpai-acf-add-on": "dev-master",
    "eteamsys/wp-all-import-pro": "dev-master",
    "eteamsys/wp-all-export-pro": "dev-master",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/acf-content-analysis-for-yoast-seo": "dev-trunk",
    "wpackagist-plugin/acf-flexible-content-modal": "dev-trunk",
    "wpackagist-plugin/advanced-custom-fields-font-awesome": "dev-trunk",
    "wpackagist-plugin/autoptimize": "dev-trunk",
    "wpackagist-plugin/breadcrumb-navxt": "*",
    "wpackagist-plugin/contact-form-7": "*",
    "wpackagist-plugin/cookie-notice": "*",
    "wpackagist-plugin/flamingo": "*",
    "wpackagist-plugin/duplicate-post": "dev-trunk",
    "wpackagist-plugin/ewww-image-optimizer": "*",
    "wpackagist-plugin/regenerate-thumbnails": "3.1.3",
    "wpackagist-plugin/redirection": "*",
    "wpackagist-plugin/simple-share-buttons-adder": "dev-trunk",
    "wpackagist-plugin/wordpress-seo": "dev-trunk",
    "wpackagist-plugin/wp-fastest-cache": "dev-trunk",
    "wpackagist-plugin/amp": "*",
    "wpackagist-plugin/classic-editor": "*",
    "wpackagist-plugin/cache-enabler": "*",
    "wpackagist-plugin/cdn-enabler": "*",
    "wpackagist-plugin/web-stories":"*",
    "eteamsys/maisonscompere_theme": "dev-feature-design-menu",
    "wpml/wpml-multilingual-cms": "*",
    "wpml/wpml-string-translation": "*",
    "wpml/wpml-media-translation": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0.2"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "remove-git-submodules" : "find . -type d -name .git | xargs rm -rf",
    "pre-install-cmd": [
        " php -r \"if(file_exists('.env') === false){copy('.env.example', '.env');}\""
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-install-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "sudo vendor/bin/wpinstall.php"
    ]
  }
}
